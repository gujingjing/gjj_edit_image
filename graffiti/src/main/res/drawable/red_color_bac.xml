<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 主体背景颜色值 -->
    <item>
        <shape>
            <solid android:color="@color/red"/>
            <stroke
                android:width="1dp"
                android:color="@color/white" />
            <!-- 圆角 -->
            <corners android:topLeftRadius="@dimen/color_radios" android:topRightRadius="@dimen/color_radios"
                android:bottomRightRadius="@dimen/color_radios" android:bottomLeftRadius="@dimen/color_radios" />

        </shape>
    </item>

</selector>
